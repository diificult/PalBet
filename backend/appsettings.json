{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Hangfire": "Information"
    }
  },
  "AllowedHosts": "*",
  "JWT": {
    "Issuer": "http://localhost:5263",
    "Audience": "http://localhost:5263"
  },
  "ConnectionStrings": {
    "Redis": "redis:6379"
  },
  "RedisRateLimits": [
    {
      "PathRegex": "/*",
      "Window": "1d",
      "MaxRequests":  1000
    }
  ]
}
